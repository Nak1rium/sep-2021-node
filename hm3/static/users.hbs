<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <style>
        * {
            margin: 0;
            padding: 0;

            box-sizing: border-box;
        }
    </style>
</head>
<body>

<div>

    {{#each users}}
        <div  style="background-color: pink; margin: 10px; padding: 10px">
            <div>{{this.firstName}}</div>
            <div>{{this.lastName}}</div>
            <div>{{this.email}}</div>
            <div>{{this.password}}</div>
            <div>{{this.age}}</div>
            <div>{{this.city}}</div>
        </div>
    {{/each}}

</div>

</body>
</html>